{
    "redirects": [
        {
            "source": "/README.md",
            "destination": "/404"
        },
        {
            "source": "/LICENSE",
            "destination": "/404"
        },
        {
            "source": "/.gitattributes",
            "destination": "/404"
        },
        {
            "source": "/.htaccess",
            "destination": "/404"
        }
    ],
    "rewrites": [
        {
            "source": "/404.html",
            "destination": "404"
        },
        {
            "source": "/index.html",
            "destination": "/"
        }
    ]
}